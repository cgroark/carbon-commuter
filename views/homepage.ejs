<div>
	<div class="text-center row form-group">
		<div class="col-md-12">
			<h1>Carbon Commute</h1>
		</div>
	</div>
</div>

<form method="post" action="/modes">
	<div class="form-group">
		<label for="start">Start:</label>
		<input type="text" name="start" placeholder="starting" class="form-control">
	</div>
	<div class="form-group">
		<label for="start">End:</label>
		<input type="text" name="end" placeholder="ending" class="form-control">
	</div>
	<div>
		<select name="mode">
		  <option value="" selected="selected">Select a Transport Mode</option>
		  <option value="bicycling">Cycling</option>
		  <option value="driving">Driving</option>
		  <option value="transit">Public Transit</option>
		  <option value="walking">Walking</option>
		</select>
	</div>
	<input type="submit" value="Submit" class="btn btn-success">
</form>
